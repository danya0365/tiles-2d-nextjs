{
  "name": "top-down-r3f",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:server": "cd my-server && npm start",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:server\"",
    "build": "next build --turbopack",
    "build:server": "cd my-server && npm run build",
    "start": "next start",
    "start:server": "cd my-server && npm start",
    "supabase-start": "supabase start",
    "supabase-stop": "supabase stop",
    "supabase-reset": "supabase reset",
    "supabase-migrate": "supabase db reset",
    "supabase-generate": "supabase gen types typescript --local --schema public > src/domain/types/supabase.ts",
    "supabase-db-diff": "supabase db diff --schema public",
    "type-check": "tsc --noEmit",
    "lint": "eslint"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@react-three/drei": "^10.7.6",
    "@react-three/fiber": "^9.4.0",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.58.0",
    "@uidotdev/usehooks": "^2.4.1",
    "axios": "^1.12.2",
    "clsx": "^2.1.1",
    "colyseus": "^0.16.5",
    "colyseus.js": "^0.16.22",
    "localforage": "^1.10.0",
    "lucide-react": "^0.544.0",
    "next": "15.5.4",
    "next-themes": "^0.4.6",
    "peerjs": "^1.5.5",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.64.0",
    "react-to-print": "^3.1.1",
    "tailwind-merge": "^3.3.1",
    "three": "^0.181.1",
    "zod": "^4.1.11",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.1.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
